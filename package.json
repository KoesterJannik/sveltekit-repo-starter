{
	"name": "sveltekit-auth-starter",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"postinstall": "npx prisma generate",
		"start": "npx prisma migrate deploy && npm run seedProducts && node -r dotenv/config build",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"seedProducts": "cross-env DOTENV_CONFIG_PATH=.env tsx -r dotenv/config prisma/seed/products.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"lucia": "^3.1.1",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"prisma": "^5.12.1",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.2.0"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-ses": "^3.540.0",
		"@internationalized/date": "^3.5.2",
		"@lucia-auth/adapter-prisma": "^4.0.1",
		"@prisma/client": "^5.11.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@types/nodemailer": "^6.4.14",
		"bits-ui": "^0.21.1",
		"clsx": "^2.1.0",
		"cmdk-sv": "^0.0.17",
		"cross-env": "^7.0.3",
		"dotenv": "^16.4.5",
		"embla-carousel-svelte": "^8.0.0",
		"formsnap": "^1.0.0",
		"lucide-svelte": "^0.364.0",
		"mode-watcher": "^0.3.0",
		"nodemailer": "^6.9.13",
		"oslo": "^1.2.0",
		"paneforge": "^0.0.2",
		"stripe": "^14.23.0",
		"svelte-radix": "^1.1.0",
		"svelte-sonner": "^0.3.21",
		"sveltekit-superforms": "^2.12.2",
		"tailwind-merge": "^2.2.2",
		"tailwind-variants": "^0.2.1",
		"tsx": "^4.7.2",
		"vaul-svelte": "^0.3.0",
		"zod": "^3.22.4"
	}
}
